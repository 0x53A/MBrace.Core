    <!--<add key="port" value="2753" />-->
    <!-- Sets a working directory for worker caching. Defaults to system temp folder. : string -->
    <!--<add key="working directory" value="/tmp/" />-->
    <!-- Specify a log file for node logging purposes. : string -->
    <add key="log file" value="log.txt" />
    <!-- Enables or disables AppDomain isolation in worker node. Defaults to true. : bool -->
    <add key="use appdomain isolation" value="true" />
